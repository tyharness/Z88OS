 shav88.o bshav88.o sshv88.o man88r.o timo88.o stim88.o

z88r: $(OBJECTSR)
	$(CC) $(CFLAGS) -o $@ $(OBJECTSR) $(LDLIBSOX)
	mv -f z88r $(DIROUT)

